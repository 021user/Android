<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

  <ImageView
      android:id="@+id/imageView13"
      android:layout_width="91dp"
      android:layout_height="76dp"
      android:layout_alignParentTop="true"
      android:layout_alignParentEnd="true"
      android:layout_marginTop="52dp"
      android:layout_marginEnd="10dp"
      android:src="@drawable/reddit" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Créer un nouveau compte"
      android:layout_marginTop="131dp"
      android:layout_marginStart="34dp" />

  <EditText
      android:id="@+id/editTextText"
      android:layout_width="292dp"
      android:layout_height="55dp"
      android:layout_below="@id/textView9"
      android:layout_marginStart="84dp"
      android:layout_marginTop="18dp"
      android:ems="10"
      android:inputType="text"
      android:text="Prénom" />

  <EditText
      android:id="@+id/editTextText2"
      android:layout_width="289dp"
      android:layout_height="50dp"
      android:layout_below="@id/editTextText"
      android:layout_marginStart="84dp"
      android:layout_marginTop="18dp"
      android:ems="10"
      android:inputType="text"
      android:text="Nom" />

  <ImageView
      android:id="@+id/imageView9"
      android:layout_width="54dp"
      android:layout_height="46dp"
      android:layout_alignTop="@id/editTextText3"
      android:layout_alignParentStart="true"
      android:layout_marginStart="12dp"
      android:layout_marginTop="1dp"
      android:src="@drawable/email" />

  <EditText
      android:id="@+id/editTextText3"
      android:layout_width="288dp"
      android:layout_height="46dp"
      android:layout_below="@id/editTextText2"
      android:layout_marginStart="84dp"
      android:layout_marginTop="17dp"
      android:ems="10"
      android:inputType="text"
      android:text="@string/prompt_email" />

  <ImageView
      android:id="@+id/imageView10"
      android:layout_width="31dp"
      android:layout_height="29dp"
      android:layout_alignTop="@id/editTextText4"
      android:layout_alignParentStart="true"
      android:layout_marginStart="21dp"
      android:layout_marginTop="15dp"
      android:src="@drawable/lock" />

  <EditText
      android:id="@+id/editTextText4"
      android:layout_width="290dp"
      android:layout_height="50dp"
      android:layout_below="@id/editTextText3"

      android:layout_alignParentStart="true"
      android:layout_marginStart="84dp"
      android:layout_marginTop="19dp"
      android:ems="10"
      android:inputType="text"
      android:text="@string/prompt_password" />

  <ImageView
      android:id="@+id/imageView11"
      android:layout_width="34dp"
      android:layout_height="34dp"
      android:layout_alignTop="@id/editTextText8"
      android:layout_alignParentStart="true"
      android:layout_marginStart="19dp"
      android:layout_marginTop="-287dp"
      android:src="@drawable/body" />

  <EditText
      android:id="@+id/editTextText8"
      android:layout_width="213dp"
      android:layout_height="41dp"
      android:layout_below="@id/editTextText7"

      android:layout_alignParentEnd="true"
      android:layout_marginTop="-40dp"
      android:layout_marginEnd="78dp"
      android:ems="10"
      android:inputType="text"
      android:text="Mobile" />

  <EditText
      android:id="@+id/editTextText7"
      android:layout_width="65dp"
      android:layout_height="41dp"
      android:layout_below="@id/editTextText4"

      android:layout_alignParentStart="true"
      android:layout_marginStart="89dp"
      android:layout_marginTop="39dp"
      android:ems="10"
      android:inputType="text|textMultiLine"
      android:text="+33" />

  <ImageView
      android:id="@+id/imageView12"
      android:layout_width="36dp"
      android:layout_height="37dp"
      android:layout_alignTop="@id/editTextText7"
      android:layout_alignParentStart="true"
      android:layout_marginStart="21dp"
      android:layout_marginTop="3dp"
      android:src="@drawable/phone" />

  <Button
      android:id="@+id/button2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_below="@id/editTextText8"
      android:layout_marginStart="172dp"
      android:layout_marginTop="40dp"
      android:backgroundTint="#FF5722"
      android:text="Confirmer" />

</RelativeLayout>
